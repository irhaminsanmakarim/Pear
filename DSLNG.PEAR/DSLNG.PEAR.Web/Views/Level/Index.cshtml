@{
    ViewBag.Title = "Level List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<hgroup class="page-hgroup-header">
    <h3 class="page-title">Level List</h3>
    <div class="breadcrumbs-regawa">
        <ul>
            <li>Plan</li>
            <li>KPI Configuration</li>
            <li>Level</li>
        </ul>
    </div>
</hgroup>

@Html.Partial("_MessageResponse")
@Html.ActionLink("New", "Create", null, new { @class = "button button-action" })
@Html.Action("IndexPartial")


@section Scripts{
    <script type="text/javascript">
        var confirmDelete = function (id) {
            if (confirm('Are you sure want to delete this item?')) {
                var url = '@Url.Action("Delete", "Level")';
                $('<form action="' + url + '" method="POST">' +
                    '<input type="hidden" name="id" value="' + id + '">' +
                    '</form>').submit();
            }
        };
    </script>
}
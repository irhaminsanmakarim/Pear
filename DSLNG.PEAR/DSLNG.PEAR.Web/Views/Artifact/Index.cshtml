@{
    ViewBag.Title = "Artifact List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="artifact-list">

    <hgroup class="page-hgroup-header">
        <h3 class="page-title">Artifacts List</h3>
        <div class="breadcrumbs-regawa">
            <ul>
                <li>Dashboard</li>
                <li>Configuration Setting</li>
                <li>Artifacts List</li>
            </ul>
        </div>
    </hgroup>

    @Html.Partial("_MessageResponse")
    @Html.ActionLink("Create New", "Designer", null, new { @class = "button button-action" })
    @Html.Action("IndexPartial")

    @section Scripts{
        <script type="text/javascript">
            var confirmDelete = function (id) {
                if (confirm('Are you sure want to delete this item?')) {
                    var url = '@Url.Action("Delete", "Measurement")';
                $('<form action="' + url + '" method="POST">' +
                    '<input type="hidden" name="id" value="' + id + '">' +
                    '</form>').submit();
            }
        };
        </script>
    }


    <!-- Modal -->
    <div class="modal fade" id="graphic-preview" tabindex="-1" role="dialog" aria-labelledby="graphicPreview" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                    <h4 class="modal-title" id="myLargeModalLabel">Artifact Preview</h4>
                </div>
                <div class="modal-body">
                    <div id="container" style="min-width: 310px; max-width: 800px; height: 400px; margin: 0 auto"></div>
                </div>
            </div>
        </div>
    </div>

</div>
